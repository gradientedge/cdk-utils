{
  "name": "@gradientedge/cdk-utils",
  "version": "0.0.0-SNAPSHOT",
  "description": "Utilities for AWS CDK provisioning",
  "main": "dist/index.js",
  "engines": {
    "node": ">=14.16.0 <15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gradientedge/cdk-utils.git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "keywords": [
    "gradientedge",
    "aws"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gradientedge/cdk-utils/issues"
  },
  "homepage": "https://github.com/gradientedge/cdk-utils#readme",
  "scripts": {
    "build": "rimraf dist/ && npx tsc",
    "ci": "yarn install --frozen-lockfile && yarn build && yarn validate && yarn docs",
    "cz": "npx cz",
    "typedocs": "rimraf api-docs/ && npx typedoc",
    "docs": "rimraf api-docs && npx jsdoc --pedantic -c jsdoc.json .",
    "fix": "yarn prettify && eslint --fix **/*.ts",
    "prettier": "npx prettier --check \"**/*.{ts,json,md}\"",
    "prettify": "npx prettier --write \"**/*.{ts,json,md}\"",
    "test": "npx jest",
    "validate": "yarn prettier && yarn test",
    "watch": "npx tsc --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettify && yarn validate"
    }
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "2.10.0-alpha.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.14",
    "app-root-path": "^3.0.0",
    "aws-cdk-lib": "^2.10.0",
    "aws-sdk": "^2.1066.0",
    "constructs": "^10.0.52",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nconf": "^0.11.3",
    "pluralize": "^8.0.0",
    "ts-node": "^10.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "aws-cdk": "*",
    "babel-eslint": "^10.1.0",
    "better-docs": "^2.7.1",
    "codecov": "^3.8.3",
    "commitizen": "^4.2.4",
    "dotenv": "^15.0.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-extended": "^2.0.0",
    "jest-junit": "^13.0.0",
    "jsdoc": "^3.6.10",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-mermaid": "^1.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.11",
    "typedoc-plugin-mermaid": "^1.6.0",
    "typescript": "4.5.5"
  },
  "optionalDependencies": {
    "@babel/core": "^7.16.12",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
