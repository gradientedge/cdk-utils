{
  "name": "@gradientedge/cdk-utils",
  "version": "0.0.0",
  "description": "Utilities for AWS CDK provisioning",
  "main": "dist/index.js",
  "engines": {
    "node": ">=12.16.0 <15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gradientedge/cdk-utils.git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "keywords": [
    "gradientedge",
    "aws"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gradientedge/cdk-utils/issues"
  },
  "homepage": "https://github.com/gradientedge/cdk-utils#readme",
  "scripts": {
    "build": "rimraf dist/ && npx tsc",
    "ci": "yarn install --frozen-lockfile && yarn build && yarn validate && yarn docs",
    "typedocs": "rimraf api-docs/ && npx typedoc",
    "docs": "rimraf api-docs && npx jsdoc --pedantic -c jsdoc.json .",
    "fix": "yarn prettify && eslint --fix **/*.ts",
    "prettier": "npx prettier --check \"**/*.{ts,json,md}\"",
    "prettify": "npx prettier --write \"**/*.{ts,json,md}\"",
    "test": "yarn build && npx jest",
    "validate": "yarn prettier && yarn test",
    "watch": "npx tsc --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettify && yarn validate"
    }
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": "coverage",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "2.2.0-alpha.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.0",
    "app-root-path": "^3.0.0",
    "aws-cdk-lib": "^2.2.0",
    "aws-sdk": "^2.1046.0",
    "constructs": "^10.0.12",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nconf": "^0.11.3",
    "pluralize": "^8.0.0",
    "ts-node": "^10.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@semantic-release/github": "^8.0.2",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "aws-cdk": "*",
    "babel-eslint": "^10.1.0",
    "better-docs": "^2.3.2",
    "dotenv": "^10.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "jest-junit": "^13.0.0",
    "jsdoc": "^3.6.7",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-mermaid": "^1.0.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^18.0.1",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typedoc-neo-theme": "^1.1.1",
    "typedoc-plugin-mermaid": "^1.6.0",
    "typescript": "4.5.4"
  },
  "optionalDependencies": {
    "@babel/core": "^7.16.5",
    "prop-types": "^15.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  }
}
