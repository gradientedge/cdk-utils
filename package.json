{
  "name": "@gradientedge/cdk-utils",
  "version": "0.0.0",
  "description": "Utilities for AWS CDK provisioning",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=16 <=22",
    "pnpm": "=10"
  },
  "packageManager": "pnpm@10.26.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gradientedge/cdk-utils.git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "keywords": [
    "gradientedge",
    "cdk",
    "cdktf",
    "awscdk"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gradientedge/cdk-utils/issues"
  },
  "homepage": "https://github.com/gradientedge/cdk-utils#readme",
  "scripts": {
    "build": "npx rimraf dist/ && npx tsc && pnpm -r build",
    "build:production": "rimraf dist/ && npx tsc -p tsconfig.prd.json && pnpm -r build",
    "ci": "pnpm install --frozen-lockfile && pnpm build && pnpm validate && pnpm run docs",
    "cz": "npx cz",
    "docs": "npx rimraf api-docs && pnpm override:plugin:docs  && npx jsdoc --pedantic -c jsdoc.json .",
    "fix": "pnpm prettify && eslint --fix **/*.ts",
    "lint": "pnpm prettify && eslint **/*.ts --cache --max-warnings=0 --no-warn-ignored",
    "override:plugin:docs": "cp theme/type-converter.js node_modules/better-docs/typescript",
    "prettier": "npx prettier --cache --check \"**/*.{ts,json,md}\"",
    "prettify": "npx prettier --cache --write \"**/*.{ts,json,md}\"",
    "test": "npx rimraf coverage && npx vitest run --coverage",
    "test:watch": "npx vitest",
    "update:deps": "ncu -u --deep --reject react,react-dom",
    "validate": "pnpm prettier && pnpm lint && pnpm test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm prettify && pnpm validate"
    }
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.953.0",
    "@aws-sdk/credential-providers": "^3.953.0",
    "@aws-sdk/types": "^3.953.0",
    "@cdktf/provider-aws": "^21.22.1",
    "@cdktf/provider-azurerm": "^14.23.1",
    "@cdktf/provider-cloudflare": "^13.9.1",
    "@types/lodash": "^4.17.21",
    "@types/node": "^25.0.3",
    "@types/uuid": "^11.0.0",
    "app-root-path": "^3.1.0",
    "aws-cdk-lib": "^2.232.2",
    "cdktf": "^0.21.0",
    "cdktf-local-exec": "^0.6.28",
    "constructs": "^10.4.4",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nconf": "^0.13.0",
    "pluralize": "^8.0.0",
    "ts-node": "^10.9.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@eslint/config-array": "^0.23.0",
    "@eslint/object-schema": "^3.0.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitest/coverage-v8": "^2.1.8",
    "aws-cdk": "^2.1100.1",
    "better-docs": "^2.7.3",
    "codecov": "^3.8.3",
    "commitizen": "^4.3.1",
    "docdash": "^2.0.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.5.0",
    "husky": "^9.1.7",
    "jsdoc": "^4.0.5",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-mermaid": "^1.0.0",
    "jsdoc-plugin-typescript": "^3.2.0",
    "jsdoc-to-markdown": "^9.1.3",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "rimraf": "^6.1.2",
    "rollup": "^4.53.5",
    "semantic-release": "^25.0.2",
    "taffydb": "^2.7.3",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3",
    "vitest": "^2.1.8",
    "yaml": "^2.8.2"
  },
  "optionalDependencies": {
    "prop-types": "^15.8.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "workspaces": [
    "app/api-destined-function/*",
    "src/*"
  ]
}
