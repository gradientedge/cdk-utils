{
  "graphqlApi": {
    "functionName": "test-graphql-server",
    "logRetention": 30,
    "memorySize": 1024,
    "timeoutInSecs": 300,
    "excludeLastModifiedTimestamp": true
  },
  "testApiDestinedLambda": {
    "functionName": "test-api-destined",
    "logRetention": 7,
    "memorySize": 1024,
    "timeoutInSecs": 60
  },
  "testLambda": {
    "functionName": "test-lambda",
    "logRetention": 7,
    "memorySize": 1024,
    "timeoutInSecs": 60,
    "tags": [
      {
        "key": "testTagName1",
        "value": "testTagValue1"
      },
      {
        "key": "testTagName2",
        "value": "testTagValue2"
      }
    ]
  },
  "testLambdaWithConcurrency": {
    "functionName": "test-lambda-concurrency",
    "logRetention": 7,
    "memorySize": 1024,
    "lambdaAlias": {
      "aliasName": "test-concurrent-alias",
      "provisionedConcurrentExecutions": 2,
      "retryAttempts": 2
    },
    "provisionedConcurrency": {
      "minCapacity": 2,
      "maxCapacity": 6,
      "utilizationTarget": 0.8
    },
    "timeoutInSecs": 60
  },
  "testLambdaWithDlq": {
    "functionName": "test-lambda-with-error-handling",
    "logRetention": 7,
    "memorySize": 1024,
    "timeoutInSecs": 60,
    "deadLetterQueueEnabled": true,
    "dlq": {
      "visibilityTimeoutInSecs": 300,
      "receiveMessageWaitTimeInSecs": 20,
      "retentionInDays": 7,
      "maxReceiveCount": 3
    },
    "redriveq": {
      "visibilityTimeoutInSecs": 300,
      "receiveMessageWaitTimeInSecs": 20,
      "retentionInDays": 7
    }
  },
  "testAnotherLambda": {
    "functionName": "test-another-lambda",
    "logRetention": 7,
    "memorySize": 1024,
    "timeoutInSecs": 60
  },
  "testLambdaEdge": {
    "functionName": "test-lambda-edge",
    "logRetention": 7,
    "memorySize": 1024,
    "timeoutInSecs": 60
  },
  "testLambdaAlias": {
    "aliasName": "test-alias",
    "provisionedConcurrentExecutions": 1
  }
}
