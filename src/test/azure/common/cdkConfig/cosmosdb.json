{
  "testCosmosDbAccount": {
    "accountName": "test-cosmosdb-account",
    "resourceGroupName": "test-rg-dev",
    "databaseAccountOfferType": "Standard",
    "consistencyPolicy": {
      "defaultConsistencyLevel": "Strong"
    },
    "locations": [
      {
        "locationName": "eastus",
        "failoverPriority": 0
      }
    ]
  },
  "testCosmosDbDatabase": {
    "databaseName": "test-cosmosdb-database",
    "accountName": "test-cosmosdb-account-dev",
    "resourceGroupName": "test-rg-dev",
    "resource": {
      "id": "test-cosmosdb-database"
    }
  },
  "testCosmosDbContainer": {
    "containerName": "test-cosmosdb-container",
    "accountName": "test-cosmosdb-account-dev",
    "databaseName": "test-cosmosdb-database-dev",
    "resourceGroupName": "test-rg-dev",
    "resource": {
      "id": "test-cosmosdb-container",
      "partitionKey": {
        "paths": ["/testPartitionKey"],
        "kind": "Hash"
      }
    },
    "indexingPolicy": {
      "includedPath": [
        {
          "path": "/*"
        }
      ],
      "excludedPath": [
        {
          "path": "/*"
        }
      ],
      "compositeIndex": [
        {
          "index": [
            {
              "path": "/assetTypeAndKey",
              "order": "Ascending"
            },
            {
              "path": "/assetType",
              "order": "Ascending"
            }
          ]
        }
      ]
    }
  }
}
